{
  "name": "tdlib-native",
  "version": "3.1.0",
  "description": "ðŸš€ Telegram TDLib native nodejs wrapper",
  "packageManager": "npm@10.9.0",
  "scripts": {
    "test": "vitest",
    "clean:dist": "rm -rf dist",
    "clean:gyp": "node-gyp clean",
    "build:types": "node scripts/generate-types.js && node scripts/generate-meta.js",
    "build:dist": "vite build",
    "build:docs": "typedoc",
    "build:gyp": "node-gyp rebuild",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*",
    "install": "node-gyp rebuild"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "type": "commonjs",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./addon": {
      "require": "./dist/addon.js",
      "import": "./dist/addon.mjs",
      "types": "./dist/addon.d.ts"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs",
      "types": "./dist/types.d.ts"
    },
    "./markup": {
      "require": "./dist/markup.js",
      "import": "./dist/markup.mjs",
      "types": "./dist/markup.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/node-tdlib.git"
  },
  "keywords": [
    "telegram",
    "tdlib",
    "tdl",
    "node",
    "backend"
  ],
  "files": [
    "dist/",
    "src/",
    "coverage/",
    "examples/",
    "binding.gyp",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE.txt",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md"
  ],
  "author": "Alexander <me@alexxgrib.me> (https://alexxgrib.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/node-tdlib/issues"
  },
  "homepage": "https://github.com/AlexXanderGrib/node-tdlib#readme",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.7.7",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "node-gyp": "^10.2.0",
    "prettier": "^3.3.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-prettier": "^4.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typedoc": "^0.26.10",
    "typedoc-plugin-markdown": "^4.2.9",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.2.4",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "debug": "^4.3.7",
    "detect-libc": "^2.0.3",
    "node-addon-api": "^8.2.1"
  },
  "optionalDependencies": {
    "@tdlib-native/tdjson-android-arm64-glibc": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b",
    "@tdlib-native/tdjson-android-arm64-musl": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b",
    "@tdlib-native/tdjson-darwin-arm64": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b",
    "@tdlib-native/tdjson-darwin-x64": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b",
    "@tdlib-native/tdjson-linux-arm64-glibc": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b",
    "@tdlib-native/tdjson-linux-arm64-musl": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b",
    "@tdlib-native/tdjson-linux-x64-glibc": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b",
    "@tdlib-native/tdjson-linux-x64-musl": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b",
    "@tdlib-native/tdjson-win32-ia32": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b",
    "@tdlib-native/tdjson-win32-x64": "1.8.37-commit.c1fea0f26355d94842f25651edc154842563e97b"
  }
}