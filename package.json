{
  "name": "tdlib-native",
  "version": "1.0.5",
  "description": "ðŸš€ Telegram TDLib native nodejs wrapper",
  "scripts": {
    "test": "jest",
    "clean:dist": "rm -rf dist",
    "clean:gyp": "node-gyp clean",
    "build:types": "node scripts/generate-types.js",
    "build:dist": "rollup -c",
    "build:docs": "typedoc",
    "build:gyp": "node-gyp rebuild",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*",
    "install": "node-gyp rebuild"
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "type": "commonjs",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./addon": {
      "require": "./addon.js",
      "import": "./addon.mjs",
      "types": "./addon.d.ts"
    },
    "./types": {
      "require": "./types.js",
      "import": "./types.mjs",
      "types": "./types.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/node-tdlib.git"
  },
  "keywords": [
    "telegram",
    "tdlib",
    "tdl",
    "node",
    "backend"
  ],
  "files": [
    "dist/",
    "src/",
    "coverage/",
    "examples/",
    "binding.gyp",
    "index.js",
    "index.mjs",
    "index.d.ts",
    "addon.js",
    "addon.mjs",
    "addon.d.ts",
    "types.d.ts",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE.txt",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md",
    "td/"
  ],
  "author": "Alexander Gribochkin <me@alexxgrib.me> (https://alexxgrib.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/node-tdlib/issues"
  },
  "homepage": "https://github.com/AlexXanderGrib/node-tdlib#readme",
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-unicorn": "^45.0.0",
    "glob": "^8.1.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "node-gyp": "^9.3.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.4.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-prettier": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.13.6",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettify",
      "pre-push": "npm run build:docs"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "node-addon-api": "^5.0.0"
  }
}
