{
  "name": "tdlib-native",
  "version": "2.3.0",
  "description": "ðŸš€ Telegram TDLib native nodejs wrapper",
  "packageManager": "npm@9.8.0",
  "scripts": {
    "test": "jest",
    "clean:dist": "rm -rf dist",
    "clean:gyp": "node-gyp clean",
    "build:types": "node scripts/generate-types.js && node scripts/generate-meta.js",
    "build:dist": "rollup -c",
    "build:docs": "typedoc",
    "build:gyp": "node-gyp rebuild",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*",
    "install": "node-gyp rebuild"
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "type": "commonjs",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./addon": {
      "require": "./addon.js",
      "import": "./addon.mjs",
      "types": "./addon.d.ts"
    },
    "./types": {
      "require": "./types.js",
      "import": "./types.mjs",
      "types": "./types.d.ts"
    },
    "./markup": {
      "require": "./markup.js",
      "import": "./markup.mjs",
      "types": "./markup.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/node-tdlib.git"
  },
  "keywords": [
    "telegram",
    "tdlib",
    "tdl",
    "node",
    "backend"
  ],
  "files": [
    "dist/",
    "src/",
    "coverage/",
    "examples/",
    "binding.gyp",
    "index.js",
    "index.mjs",
    "index.d.ts",
    "addon.js",
    "addon.mjs",
    "addon.d.ts",
    "types.js",
    "types.mjs",
    "types.d.ts",
    "markup.js",
    "markup.mjs",
    "markup.d.ts",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE.txt",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md"
  ],
  "author": "Alexander <me@alexxgrib.me> (https://alexxgrib.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexxgrib/node-tdlib/issues"
  },
  "homepage": "https://github.com/alexxgrib/node-tdlib#readme",
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.7",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.19.34",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-unicorn": "^46.0.1",
    "glob": "^9.3.5",
    "jest": "^29.7.0",
    "node-gyp": "^9.4.1",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.1",
    "rollup": "^3.29.4",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-prettier": "^3.1.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "debug": "^4.3.5",
    "node-addon-api": "^6.1.0",
    "typedoc": "^0.25.13"
  },
  "optionalDependencies": {
    "@tdlib-native/tdjson-darwin-x64": "1.8.30-commit.4257a341bd08c1fa4d5329b7ecc75cf15563aa23",
    "@tdlib-native/tdjson-linux-x64-glibc": "1.8.30-commit.4257a341bd08c1fa4d5329b7ecc75cf15563aa23",
    "@tdlib-native/tdjson-win32-x64": "1.8.30-commit.4257a341bd08c1fa4d5329b7ecc75cf15563aa23"
  }
}
