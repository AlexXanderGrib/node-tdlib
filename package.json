{
  "name": "tdlib-native",
  "version": "2.7.0",
  "description": "ðŸš€ Telegram TDLib native nodejs wrapper",
  "packageManager": "npm@10.9.0",
  "scripts": {
    "test": "jest",
    "clean:dist": "rm -rf dist",
    "clean:gyp": "node-gyp clean",
    "build:types": "node scripts/generate-types.js && node scripts/generate-meta.js",
    "build:dist": "rollup -c",
    "build:docs": "typedoc",
    "build:gyp": "node-gyp rebuild",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*",
    "install": "node-gyp rebuild"
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "type": "commonjs",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./addon": {
      "require": "./addon.js",
      "import": "./addon.mjs",
      "types": "./addon.d.ts"
    },
    "./types": {
      "require": "./types.js",
      "import": "./types.mjs",
      "types": "./types.d.ts"
    },
    "./markup": {
      "require": "./markup.js",
      "import": "./markup.mjs",
      "types": "./markup.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/node-tdlib.git"
  },
  "keywords": [
    "telegram",
    "tdlib",
    "tdl",
    "node",
    "backend"
  ],
  "files": [
    "dist/",
    "src/",
    "coverage/",
    "examples/",
    "binding.gyp",
    "index.js",
    "index.mjs",
    "index.d.ts",
    "addon.js",
    "addon.mjs",
    "addon.d.ts",
    "types.js",
    "types.mjs",
    "types.d.ts",
    "markup.js",
    "markup.mjs",
    "markup.d.ts",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE.txt",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md"
  ],
  "author": "Alexander <me@alexxgrib.me> (https://alexxgrib.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexxgrib/node-tdlib/issues"
  },
  "homepage": "https://github.com/alexxgrib/node-tdlib#readme",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.6",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.12.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "node-gyp": "^10.2.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-prettier": "^4.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.10",
    "typedoc-plugin-markdown": "^4.2.9",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "debug": "^4.3.7",
    "node-addon-api": "^8.2.1"
  },
  "optionalDependencies": {
    "@tdlib-native/tdjson-darwin-x64": "1.8.37-commit.21e5ce0e977fe012e0cee4e6fcfe704b47b00774",
    "@tdlib-native/tdjson-darwin-arm64": "1.8.37-commit.21e5ce0e977fe012e0cee4e6fcfe704b47b00774",
    "@tdlib-native/tdjson-linux-x64-glibc": "1.8.37-commit.21e5ce0e977fe012e0cee4e6fcfe704b47b00774",
    "@tdlib-native/tdjson-win32-x64": "1.8.37-commit.21e5ce0e977fe012e0cee4e6fcfe704b47b00774"
  }
}
