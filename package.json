{
  "name": "tdlib-native",
  "version": "3.8.0",
  "description": "ðŸš€ Telegram TDLib native nodejs wrapper",
  "packageManager": "npm@11.5.2",
  "config": {
    "tdlib_prebuilt_repository": "AlexXanderGrib/prebuilt-tdlib",
    "tdlib_prebuilt_tag": "0.1.8.52-18f6c78"
  },
  "scripts": {
    "test": "vitest",
    "clean:dist": "rm -rf dist",
    "clean:gyp": "node-gyp clean",
    "sync-tdlib": "node scripts/prepack-binary.js && node scripts/generate-types.js && node scripts/generate-meta.js",
    "build:dist": "vite build",
    "build:docs": "typedoc",
    "build:gyp": "node-gyp rebuild",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "type": "commonjs",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./td.node": {
      "types": "./td.d.ts",
      "require": "./build/Release/td.node",
      "import": null
    },
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./addon": {
      "types": "./dist/addon.d.ts",
      "require": "./dist/addon.js",
      "import": "./dist/addon.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./markup": {
      "types": "./dist/markup.d.ts",
      "require": "./dist/markup.js",
      "import": "./dist/markup.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/node-tdlib.git"
  },
  "keywords": [
    "telegram",
    "tdlib",
    "tdl",
    "node",
    "backend"
  ],
  "files": [
    "addon/",
    "dist/",
    "src/",
    "coverage/",
    "examples/",
    "binding.gyp",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE.txt",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md",
    "td.d.ts"
  ],
  "author": "Alexander <me@alexxgrib.me> (https://alexxgrib.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/node-tdlib/issues"
  },
  "homepage": "https://github.com/AlexXanderGrib/node-tdlib#readme",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.3.0",
    "detect-libc": "^2.0.4",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^60.0.0",
    "node-gyp": "^11.3.0",
    "prettier": "^3.6.2",
    "typedoc": "^0.28.10",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "debug": "^4.4.1",
    "node-addon-api": "^8.5.0"
  },
  "optionalDependencies": {
    "@tdlib-native/tdjson": "1.8.52-commit.18f6c78cfb736cb24db789534e7ff3d274df77b3"
  }
}
